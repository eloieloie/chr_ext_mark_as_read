{
    "name": "Mark as Visited",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "hello_extensions.png"
    },
    "permissions": ["tabs", "storage", "history"],
    "background": {
      "service_worker": "background.js"
    }
  }